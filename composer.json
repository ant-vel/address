{
    "license": "MIT",
    "name": "antvel/address",
    "homepage": "http://antvel.com",
    "type": "laravel-bundle",
    "description": "The Antvel e-commerce address componet.",
    "keywords": [
        "laravel",
        "address",
        "component",
        "antvel"
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Gustavo Ocanto",
            "email": "gustavoocanto@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/ant-vel/address",
        "issues": "https://github.com/ant-vel/address/issues"
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/container": "5.6.*",
        "illuminate/contracts": "5.6.*",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*",
        "mockery/mockery": "~1.0",
        "sebastian/diff": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Antvel\\Address\\": "src/Address/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antvel\\Address\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antvel\\AntvelServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}